    "body-parser": "^1.15.1",
    "coveralls": "^2.11.11",
    "express": "^4.13.4",
    "hapi": "^13.4.1",
    "istanbul": "^0.4.3",
    "jsdoc": "git+https://github.com/jsdoc3/jsdoc.git",
    "jshint": "^2.9.2",
    "koa": "^1.2.0",
    "nock": "^8.0.0",
    "restify": "^4.1.0",
    "tape": "^4.5.1"
  },
  "dependencies": {
    "@google/cloud-diagnostics-common": "0.3.0",
    "lodash": "^4.13.1"
